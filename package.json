{
  "name": "jest-playwright-preset",
  "version": "1.4.2",
  "main": "index.js",
  "description": "Running tests using Jest & Playwright.",
  "license": "MIT",
  "keywords": [
    "jest",
    "playwright",
    "jest-playwright",
    "jest-playwright-preset"
  ],
  "author": "playwright-community",
  "bugs": {
    "url": "https://github.com/playwright-community/jest-playwright/issues"
  },
  "types": "./types/global.d.ts",
  "repository": {
    "url": "https://github.com/playwright-community/jest-playwright.git",
    "type": "git"
  },
  "homepage": "https://github.com/playwright-community/jest-playwright#readme",
  "scripts": {
    "start": "npm run build -- --watch",
    "build": "tsc",
    "format": "prettier --write \"*.{js,md}\" \"src/**/*.js\"",
    "lint": "tsc --noEmit && eslint . --ext .js,.ts",
    "prepublishOnly": "npm run build",
    "test": "npm run test:src && npm run test:e2e",
    "test:src": "jest",
    "test:e2e": "jest -c jest.config.e2e.js",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,md}": [
      "prettier --write"
    ],
    "*.{js,ts}": "eslint --fix"
  },
  "dependencies": {
    "expect-playwright": "^0.2.6",
    "jest-circus": "^26.6.3",
    "jest-environment-node": "^26.6.2",
    "jest-process-manager": "^0.2.7",
    "jest-runner": "^26.6.3",
    "nyc": "^15.1.0",
    "playwright-core": ">=1.2.0",
    "rimraf": "^3.0.2",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@types/debug": "4.1.5",
    "@types/jest": "26.0.19",
    "@types/jest-dev-server": "4.2.0",
    "@types/node": "14.14.13",
    "@types/rimraf": "^3.0.0",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "4.11.0",
    "@typescript-eslint/parser": "4.9.0",
    "coveralls": "3.1.0",
    "eslint": "7.16.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "7.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.3.0",
    "husky": "4.3.6",
    "jest": "26.6.3",
    "lint-staged": "10.5.3",
    "playwright": ">=1.2.0",
    "playwright-chromium": ">=1.2.0",
    "prettier": "2.2.1",
    "ts-jest": "26.4.4",
    "typescript": "4.1.3"
  }
}
